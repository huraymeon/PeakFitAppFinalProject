<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="384.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.yourteam.app.peakfitappfinalproject.model.WorkoutHistoryController">
   <children>
      <VBox alignment="CENTER" layoutX="105.0" layoutY="100.0" prefHeight="200.0" prefWidth="192.0" spacing="15.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" text="Workout History">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <TextArea fx:id="logArea" prefHeight="136.0" prefWidth="192.0" />
            <Button fx:id="loadLog" mnemonicParsing="false" onAction="#loadLog" text="Load" />
            <Button mnemonicParsing="false" onAction="#handleBackToMain" text="Back" />
         </children>
      </VBox>
   </children>
</AnchorPane>
